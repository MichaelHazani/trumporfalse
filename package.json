{
  "name": "trumporfalse",
  "version": "1.0.0",
  "description": "Trump or False: A Markov Chain Gamesperiment",
  "main": "app.js",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "start": "npm run server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-js": "make watch-js",
    "build-libs": "make build-libs",
    "build-js": "make build-js",
    "build-server": "babel src/server --out-dir build/server",
    "build-assets": "make build-assets",
    "clean": "make clean",
    "server": "node server.js",
    "serve-build": "npm run build-server && npm run build-libs && npm run build-assets && npm run build-js",
    "heroku-postbuild": "npm run serve-build",
    "markov-watch": "watchify -v -e src/server/markovGenerator.js -t [ babelify --presets [ es2015 react ] ] -o build/server/markovGenerator.js"
  },
  "author": "Leo Langinger & Michael Hazani",
  "license": "MIT",
