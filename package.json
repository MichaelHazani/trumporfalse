    "async": "^2.1.1",
    "axios": "^0.15.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "classnames": "^2.2.5",
    "deepfreeze": "^2.0.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "fela": "^3.0.1",
    "gulp": "^3.9.1",
    "markovchain": "^1.0.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fela": "^3.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sassify": "^2.0.0",
    "twitter": "^1.4.0",
    "twitter-node-client": "0.0.6",
    "underscore": "^1.8.3",
    "watchify": "^3.7.0",
    "webpack": "^1.13.2"
  }
